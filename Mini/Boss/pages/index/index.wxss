@import '/style/weui.wxss';

.page{    
    font-size: 16px;
    font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
    color: #efefef;
}
.page1{background-color: #278296;}
.page__hd {
    padding: 15px 40px;
}
.page1 .page__bd {
    padding-bottom: 5px;
}
.page__bd {
    padding-bottom: 20px;
}
.page__bd_spacing {
    padding-left: 15px;
    padding-right: 15px;
}
.page2 .weui-grid{text-align: center;}
.page2 .weui-grid{border-color: #efefef;}
.page .iconfont{
  font-size: 2rem;
  color: #278296;
  text-align: center;
}
.page__ft{
    text-align: center;
    padding:0 0 10px;
    padding:0 0 calc(10px + constant(safe-area-inset-bottom));
    padding:0 0 calc(10px + env(safe-area-inset-bottom));
}
.page1 .weui-grids{border:0;}
.page1 .weui-grid{border:0;}
.page1 .weui-grid__label{color: #efefef;}
.page2 .weui-grid__label{font-size: 12px;}
.page__title {
    text-align: left;
    font-size: 18px;
    font-weight: 400;
}
.page1 .page__desc {
    margin-top: 5px;
    color: #efefef;
    text-align: left;
    font-size: 14px;
}
.weui-cell_example:before{
    left:52px;
}
.tip-msg{
  height: 40px;
  line-height: 40px;
  padding: 0 10px;
  font-size: 11px;
  color: #565656;
}
